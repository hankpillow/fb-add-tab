{
	"name": "FB add tab."
	, "version": "1.0"
	, "manifest_version": 2
	, "description": "This extension helps you to add an app as tab in facebook."
	, "icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
	, "browser_action": {
    	"default_icon": "icon16.png"
		, "default_title": "Adding Facebook tab without hassle!"
		, "default_popup": "popup.html"
    }
	, "permissions": ["tabs", "http://*/*", "https://*/*"]
}